package main;

public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Graph graph = new Graph(10);
		graph.generateGraph();
		graph.solveHamiltonianPathRecursively();
		for(Edge e:graph.getResult())
		{
			System.out.println("Node src: "+e.getSrc().getValue()+", "+e.getDest().getValue());
		}
	}

}
