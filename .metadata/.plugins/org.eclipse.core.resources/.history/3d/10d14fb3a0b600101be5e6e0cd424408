package main;

import java.util.Map;
import java.util.Set;


public class Test
{

	public static void main(String [] args)
	{
		Graph g = new Graph();
		Map<Graph.Node, Integer> mapping = g.color(2);
		Set<Graph.Node> nodes = mapping.keySet();
		for(Graph.Node n: nodes)
		{
			System.out.println("Node: "+n.label+", color: "+mapping.get(n));
		}
	}
}