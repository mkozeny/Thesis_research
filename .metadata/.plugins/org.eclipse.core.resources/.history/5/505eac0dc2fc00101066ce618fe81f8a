package main;

public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int [] v = new int[10];
		v[0]=6;
		v[1]=2;
		v[2]=8;
		v[3]=1;
		v[4]=44;
		v[5]=7;
		v[6]=28;
		v[7]=5;
		v[8]=16;
		v[9]=9;
		

		int [] c = new int[10];
		c[0]=247;
		c[1]=210;
		c[2]=163;
		c[3]=93;
		c[4]=255;
		c[5]=244;
		c[6]=190;
		c[7]=117;
		c[8]=234;
		c[9]=105;
		
		KnapSack kn = new KnapSack(230,10, c, v);
		
		Boolean [] result = kn.solveKnapSackProblem(512);
		for(int i=0; i < result.length; i++)
		{
			System.out.print((result[i]?1:0)+" ");
		}
	}

}
