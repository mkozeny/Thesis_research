		KnapSack kn = new KnapSack(200, 15);
		int [] v = new int[15];
		v[0]=18;
		v[1]=42;
		v[2]=88;
		v[3]=3;
		int [] c = new int[15];
		c[0]=114;
		c[1]=136;
		c[2]=192;
		c[3]=223;
		kn.setV(v);
		kn.setC(c);
		Integer [] result = kn.solveKnapSackProblem(2350);
		for(int i=0; i < result.length; i++)
		{
			System.out.print(result[i]+" ");
		}
	}

}
