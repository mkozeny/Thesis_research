package game;

public class Cell {

	public Cell() {
		super();
	}
	
	public Cell(int row, int column, int free) {
		super();
		this.row = row;
		this.column = column;
		this.free = free;
	}

	private int row;
	
	private int column;
	
	private int free;

	public int getRow() {
		return row;
	}

	public void setRow(int row) {
		this.row = row;
	}

	public int getColumn() {
		return column;
	}

	public void setColumn(int column) {
		this.column = column;
	}

	public int getFree() {
		return free;
	}

	public void setFree(int free) {
		this.free = free;
	}
	
	
}
