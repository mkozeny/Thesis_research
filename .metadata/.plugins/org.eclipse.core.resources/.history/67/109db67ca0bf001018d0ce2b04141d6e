package main;

public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		NQueensProblemSolver solver = new NQueensProblemSolver(8);
		solver.solveNQueensProblemRecursively(0, 0);
		for(Cell cell:solver.getCells())
		{
			System.out.println();
		}
	}

	
	
}
