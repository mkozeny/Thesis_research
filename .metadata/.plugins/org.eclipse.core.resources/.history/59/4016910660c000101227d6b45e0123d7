package main;

public class Edge {
    private Node src;
	private Node dest;
    public Edge(Node src, Node dest) {
        this.src = src;
        this.dest = dest;
        
    }
	public Node getSrc() {
		return src;
	}
	public void setSrc(Node src) {
		this.src = src;
	}
	public Node getDest() {
		return dest;
	}
	public void setDest(Node dest) {
		this.dest = dest;
	} 
    
    
  }

