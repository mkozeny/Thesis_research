package game;

import java.util.List;
import game.Cell;

import edu.mit.csail.sdg.annotations.Ensures;
import edu.mit.csail.sdg.annotations.Modifies;
import edu.mit.csail.sdg.squander.Squander;
import edu.mit.csail.sdg.squander.annotations.FreshObjects;

public class SoliterSolver {

	
	
	
	public SoliterSolver(List<State> states) {
		super();
		this.states = states;
	}


	List<State> states;
	
	
	//@Ensures({"return[int] in this.states.elts"})
	@Modifies({"Cell.free [] []","return.length", "return.elems"})
	@FreshObjects(cls = State[].class, num = 1)
	public State [] solveSolitergame(Cell c)
	{
		return Squander.exe(this,c);
	}

	
	
	
}
